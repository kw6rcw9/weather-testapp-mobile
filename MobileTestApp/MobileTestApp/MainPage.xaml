<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileTestApp.MainPage">

    <StackLayout>
        
        
        
        <Entry IsPassword="True" Placeholder="Enter password"/>
        <Entry Placeholder="Enter email"/>
        <Editor Placeholder="Enter text"/>
        <Slider Minimum="10" Maximum="50" VerticalOptions="CenterAndExpand"/>
        <Switch IsToggled="True" />
        <WebView Source="https://google.com" HorizontalOptions="Center" VerticalOptions="CenterAndExpand"/>


    </StackLayout>

</ContentPage>
